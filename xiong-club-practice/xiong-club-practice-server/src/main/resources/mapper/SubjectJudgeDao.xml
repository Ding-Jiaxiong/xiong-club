<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dingjiaxiong.practice.server.dao.SubjectJudgeDao">

    <select id="selectBySubjectId"
            resultType="com.dingjiaxiong.practice.server.entity.po.SubjectJudgePO">
        select  is_correct as isCorrect
        from subject_judge
        where is_deleted = 0
          and subject_id = #{subjectId,jdbcType=BIGINT}
    </select>

</mapper>
